		bCompress : false,				
	};
	 var save=confirm("데이터를 입력하시겠습니까?");
	if(save){
		//공통 transaction 함수 호출(콜백함수)
		Iject.transaction(this,oDatas,function(){
		if(this.errorcode < 0)
			{	
			   Iject.alert(this.errormsg);
			   return;
			}
		else{
			alert("입력완료");
		}
		});	
	}else{
		alert("입력을 취소하셨습니다.");
	}
}

